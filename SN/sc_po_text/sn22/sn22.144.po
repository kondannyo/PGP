msgctxt "sn22.144:0.1"
msgid "Saṃyutta Nikāya 22 "
msgstr "Linked Discourses 22 "

#. HTML: </p><p>
msgctxt "sn22.144:0.2"
msgid "14. Kukkuḷavagga "
msgstr "14. Burning Chaff "

#. HTML: </p><h1>
msgctxt "sn22.144:0.3"
msgid "144. Dutiyaanattasutta "
msgstr "144. Not-Self (2nd) "

#. HTML: </h1></div><p>
#. REF: pts-vp-pli3.179, sc1
msgctxt "sn22.144:1.1"
msgid "Sāvatthinidānaṃ. "
msgstr "At Sāvatthī. "

#. HTML: </p><p>
msgctxt "sn22.144:1.2"
msgid "“Yo, bhikkhave, anattā; tatra vo rāgo pahātabbo. "
msgstr "“Mendicants, you should give up greed for what is not-self. …” "

msgctxt "sn22.144:1.3"
msgid "Ko ca, bhikkhave, anattā? "
msgstr " "

msgctxt "sn22.144:1.4"
msgid "Rūpaṃ, bhikkhave, anattā; tatra vo rāgo pahātabbo. "
msgstr " "

msgctxt "sn22.144:1.5"
msgid "Vedanā anattā … "
msgstr " "

msgctxt "sn22.144:1.6"
msgid "saññā … "
msgstr " "

msgctxt "sn22.144:1.7"
msgid "saṅkhārā … "
msgstr " "

msgctxt "sn22.144:1.8"
msgid "viññāṇaṃ anattā; tatra vo rāgo pahātabbo. "
msgstr " "

msgctxt "sn22.144:1.9"
msgid "Yo, bhikkhave, anattā; tatra vo rāgo pahātabbo”ti. "
msgstr " "

#. HTML: </p><p class="endsection">
msgctxt "sn22.144:1.10"
msgid "Navamaṃ. "
msgstr ""

