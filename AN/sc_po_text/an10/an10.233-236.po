msgctxt "an10.233-236:0.1"
msgid "Aṅguttara Nikāya 10 "
msgstr "Numbered Discourses 10 "

#. HTML: </p><p>
msgctxt "an10.233-236:0.2"
msgid "22. Sāmaññavagga "
msgstr "22. Similarity "

#. HTML: </p><h1>
msgctxt "an10.233-236:0.3"
msgid "233–236 "
msgstr "233–236. "

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "an10.233-236:1.1"
msgid "“Dasahi, bhikkhave, dhammehi samannāgato bālo veditabbo … pe … "
msgstr "“A fool is known by ten qualities … "

msgctxt "an10.233-236:1.2"
msgid "paṇḍito veditabbo … pe … "
msgstr "astute person … "

msgctxt "an10.233-236:1.3"
msgid "vīsatiyā, bhikkhave … pe … "
msgstr "twenty … "

msgctxt "an10.233-236:1.4"
msgid "tiṃsāya, bhikkhave … pe … "
msgstr "thirty … "

msgctxt "an10.233-236:1.5"
msgid "cattārīsāya, bhikkhave, dhammehi samannāgato bālo veditabbo … pe … "
msgstr "forty …” "

msgctxt "an10.233-236:1.6"
msgid "paṇḍito veditabbo … pe … "
msgstr " "

msgctxt "an10.233-236:1.7"
msgid "imehi kho, bhikkhave, cattārīsāya dhammehi samannāgato paṇḍito veditabbo”ti. "
msgstr " "

#. HTML: </p><p class="endsection">
msgctxt "an10.233-236:1.8"
msgid "Sāmaññavaggo dutiyo. "
msgstr ""

#. HTML: </p></article></section></body></html>
